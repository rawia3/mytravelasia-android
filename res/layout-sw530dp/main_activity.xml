<?xml version="1.0" encoding="utf-8"?>

<android.support.v4.widget.DrawerLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/dl_main"
        android:orientation="vertical"
        style="@style/LayoutMatchMatch">
    <RelativeLayout style="@style/LayoutMatchMatch">
        <FrameLayout android:id="@+id/fl_list_container"
                     style="@style/LayoutMatchMatch"/>
        <FrameLayout android:id="@+id/fl_map_container"
                     android:layout_toRightOf="@id/fl_list_container"
                     style="@style/LayoutMatchMatch"/>
    </RelativeLayout>
    <include layout="@layout/side_nav_include"/>
</android.support.v4.widget.DrawerLayout>